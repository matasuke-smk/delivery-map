{
  "name": "配達マップ",
  "short_name": "配達マップ",
  "description": "配達ドライバー向けの地図ナビゲーションアプリ",
  "start_url": "/delivery-map/",
  "scope": "/delivery-map/",
  "display": "standalone",
  "orientation": "portrait",
  "theme_color": "#0066CC",
  "background_color": "#FFFFFF",
  "icons": [
    {
      "src": "/delivery-map/icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "/delivery-map/vite.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "url_handlers": [
    {
      "origin": "https://www.google.com"
    },
    {
      "origin": "https://maps.google.com"
    },
    {
      "origin": "https://goo.gl"
    }
  ],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["focus-existing", "auto"]
  },
  "shortcuts": [
    {
      "name": "ナビゲーション開始",
      "short_name": "ナビ開始",
      "url": "/delivery-map/?action=navigate"
    }
  ],
  "share_target": {
    "action": "/delivery-map/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+deliverymap",
      "url": "/delivery-map/?url=%s"
    }
  ]
}